<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <servlet>
  	<servlet-name>add</servlet-name>
  	<servlet-class>com.kadeGroup.AddServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>square</servlet-name>
  	<servlet-class>com.kadeGroup.SquareServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>login</servlet-name>
  	<servlet-class>com.kadeGroup.LoginServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>add</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>square</servlet-name>
  	<url-pattern>/square</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  
</web-app>